    DOSQCURDIR                @71
    DOSQCURDISK               @72
    DOSQFILEINFO              @74
    DOSQFILEMODE              @75
    DOSREAD                   @137
    DOSWRITE                  @138
    DOS32FLATDS               @370
    DOS32FLATCS               @369
    DOSIODELAYCNT             @427
